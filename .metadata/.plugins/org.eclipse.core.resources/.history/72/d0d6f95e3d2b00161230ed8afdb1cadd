package com.intw.practice.array;
import java.util.*;
import java.lang.*;
import java.io.*;

class TripletSum {
	public static void main (String[] args) {
		//code
	    Scanner reader = new Scanner(System.in);
	    int numOfTestcase = reader.nextInt();
	    while(numOfTestcase > 0){
	        int arrSize = reader.nextInt();
	        int sum = reader.nextInt();
	        int []arr = new int[arrSize];
	        for(int i = 0; i < arrSize; i++){
	            arr[i] = reader.nextInt();
	        }
	        
	        Arrays.sort(arr);
	        boolean found = false;
	        for(int j =0; j < arrSize-2; j++){
	        	int first = j;
	        	int second =  j+1;
	        	int last = arrSize-1;
	            while(first <= last){
	                if(arr[first] + arr[second] + arr[last] == sum){
	                	found = true;
	                }
	                
	                if(arr[first] + arr[second] + arr[last] > sum){
	                last = last -1;
	                }
	                else{
	                	first = first +1;
	                }
	                
	            }
	            
	        }
	        if(found){
	        	 System.out.println("1");
	        }
	        else{
	        	System.out.println("-1");
	        }
	     
	        numOfTestcase = numOfTestcase-1;

	    }
	   	    
	    
	}
	
	
}